<h1>注文履歴一覧</h1>
<table>
    <thead>
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>
    </thead>
    <% @orders.each do |f| %>
    <tbody>
        <tr>
            <td>
                <%= link_to admin_path(f.id) do %>
                  <%= f.created_at.strftime("%Y/%m/%d %H:%M:%S") %>
                <% end %>
            </td>
            <td><%= f.customer.last_name %><%= f.customer.first_name %></td>
            <td><%= f.order_details.sum(:amount) %></td>
            <td><%= f.status_i18n %></td>
        </tr>
    </tbody>
    <% end %>
</table>